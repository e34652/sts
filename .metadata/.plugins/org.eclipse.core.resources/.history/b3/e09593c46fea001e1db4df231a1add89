		<![CDATA[
new Dept(rs.getInt("DEPTNO"), rs.getString("DNAME"), rs.getString("LOC"));
		
			SELECT
				DEPTNO,
				DNAME,
				LOC,
			FROM DEPT
			
		]]>
	</select>

